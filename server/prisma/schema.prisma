generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Quote {
  id          String   @id @default(uuid()) @db.Uuid
  text        String
  fingerprint String   @unique
  elo         Int      @default(1200)
  wins        Int      @default(0)
  losses      Int      @default(0)
  voteCount   Int      @default(0)
  createdAt   DateTime @default(now())
}
